#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ahmed/github/chat_app/server/node_modules/.pnpm/typeorm@0.3.14_mysql2@3.2.1_ts-node@10.9.1/node_modules/typeorm/node_modules:/home/ahmed/github/chat_app/server/node_modules/.pnpm/typeorm@0.3.14_mysql2@3.2.1_ts-node@10.9.1/node_modules:/home/ahmed/github/chat_app/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ahmed/github/chat_app/server/node_modules/.pnpm/typeorm@0.3.14_mysql2@3.2.1_ts-node@10.9.1/node_modules/typeorm/node_modules:/home/ahmed/github/chat_app/server/node_modules/.pnpm/typeorm@0.3.14_mysql2@3.2.1_ts-node@10.9.1/node_modules:/home/ahmed/github/chat_app/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../../cli-ts-node-esm.js" "$@"
fi
