#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ahmed/github/chat_app/node_modules/.pnpm/ts-jest@29.0.5_@babel+core@7.21.4_jest@29.5.0_typescript@4.7.4/node_modules/ts-jest/node_modules:/home/ahmed/github/chat_app/node_modules/.pnpm/ts-jest@29.0.5_@babel+core@7.21.4_jest@29.5.0_typescript@4.7.4/node_modules:/home/ahmed/github/chat_app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ahmed/github/chat_app/node_modules/.pnpm/ts-jest@29.0.5_@babel+core@7.21.4_jest@29.5.0_typescript@4.7.4/node_modules/ts-jest/node_modules:/home/ahmed/github/chat_app/node_modules/.pnpm/ts-jest@29.0.5_@babel+core@7.21.4_jest@29.5.0_typescript@4.7.4/node_modules:/home/ahmed/github/chat_app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
